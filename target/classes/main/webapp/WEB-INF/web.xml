<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>mid-exam</display-name>
  <welcome-file-list>
    <welcome-file>signin1.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>GetLocationsServlet</servlet-name>
    <servlet-class>com.auca.library.servlet.GetLocationsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetLocationsServlet</servlet-name>
    <url-pattern>/getLocations</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddLocationServlet</servlet-name>
    <servlet-class>com.auca.library.servlet.AddLocationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddLocationServlet</servlet-name>
    <url-pattern>/addLocation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewBooksServlet</servlet-name>
    <servlet-class>com.auca.library.servlet.ViewBooksServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewBooksServlet</servlet-name>
    <url-pattern>/ViewBooksServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BooksServlet</servlet-name>
    <servlet-class>BooksServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BooksServlet</servlet-name>
    <url-pattern>/books</url-pattern>
  </servlet-mapping>
</web-app>